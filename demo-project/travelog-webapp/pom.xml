    <groupId>kgi.presentations.k8s</groupId>
    <artifactId>travelog-ws</artifactId>
    <packaging>jar</packaging>
    <version>1.0-SNAPSHOT</version>


    <properties>
        <awaitility.version>1.7.0</awaitility.version>
        <aws-sdk.version>1.11.86</aws-sdk.version>
        <aspectj-maven-plugin.version>1.9</aspectj-maven-plugin.version>
        <aspectj-runtime.version>1.8.9</aspectj-runtime.version>
        <commons-io.version>2.5</commons-io.version>
        <dbcp2.version>2.1.1</dbcp2.version>
        <ehcache.version>2.9.1</ehcache.version>
